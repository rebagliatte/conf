// To regenerate
// 1. Download from https://icomoon.io/app/#/projects
// 2. Copy over fonts and replace this file
// 2. Remove '/fonts' from the src declarations on this file

@font-face {
  font-family: 'icomoon';
  src:url('icomoon.eot?edkgsh');
  src:url('icomoon.eot?#iefixedkgsh') format('embedded-opentype'),
    url('icomoon.woff?edkgsh') format('woff'),
    url('icomoon.ttf?edkgsh') format('truetype'),
    url('icomoon.svg?edkgsh#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  font-family: 'icomoon';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

